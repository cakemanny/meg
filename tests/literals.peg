
parse: "hello"

%%

let () =
  let input1 = string_view_of_string "hallo there darling" in
  match (parse input1) with
| Success (value, remaining_input) ->
    let (str,off,len) = remaining_input in
    Printf.printf "Success: %s, %d char(s) of input remaining\n" value len
| Error msg -> Printf.printf "No match %s" msg

